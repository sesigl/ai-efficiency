{
  "name": "ai-efficiency-benchmark",
  "version": "1.0.0",
  "description": "Measuring agentic AI coding performance with API-level test specifications",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "biome lint src",
    "format": "biome format --write src",
    "format:check": "biome format src",
    "knip": "knip",
    "ls-lint": "ls-lint",
    "verify": "npm run format:check && npm run lint && npm run ls-lint && npm run knip && npm run build && npm run test"
  },
  "keywords": [
    "typescript",
    "fastify",
    "ai-benchmark"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "fastify": "^4.25.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@ls-lint/ls-lint": "^2.3.1",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.6.1",
    "knip": "^5.83.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
